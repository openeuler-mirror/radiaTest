    "@vicons/carbon": "^0.11.0",
    "@vicons/fa": "^0.10.0",
    "@vicons/fluent": "^0.10.0",
    "@vicons/ionicons4": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vicons/material": "^0.10.0",
    "@vicons/tabler": "^0.10.0",
